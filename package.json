  "bugs": {
    "url": "https://github.com/brianjenkins94/Rosettas-Node/issues"
  },
  "homepage": "https://github.com/brianjenkins94/Rosettas-Node#readme",
  "dependencies": {
    "minimist": "latest",
    "ts-node": "latest",
    "typescript": "latest"
  },
  "devDependencies": {
    "@types/minimist": "latest",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest"
  }
